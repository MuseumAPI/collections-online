<!DOCTYPE html>
<html>
    <head>                        
        @Html.Partial("Metadata")        
        <link href="/content/css/styles.css" rel="stylesheet">        
        <script type="text/javascript" src="//use.typekit.net/izq0fed.js"></script>
        <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
        @if (IsSectionDefined("Head"))
        {
            @RenderSection("Head")
        }
        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
          ga('create', 'UA-11442629-1', 'auto');
          //ga('send', 'pageview');
        </script>
    </head>
    <body>
        @Html.Partial("Header")
        @RenderBody()
        <footer>
            <div class="wrapper">
                <div class="logos">
                    <img alt="Museum Victoria" src="/content/img/logo-mv.png">
                </div>
                <div class="footerpromos">
                    <div class="footerpromo first">
                        <h2>Visit Us</h2>
                        <ul>
                            <li><a href="http://museumvictoria.com.au/melbournemuseum/">Melbourne Museum</a></li>
                            <li><a href="http://museumvictoria.com.au/scienceworks/">Scienceworks</a></li>
                            <li><a href="http://museumvictoria.com.au/immigrationmuseum/">Immigration Museum</a></li>
                            <li><a href="http://museumvictoria.com.au/reb/">Royal Exhibition Building</a></li>
                        </ul>
                    </div>
                    <div class="footerpromo">
                        <h2>Memberships</h2>
                        <ul>
                            <li><a href="http://museumvictoria.com.au/about/members/how-to-join/">Join MV members</a></li>
                            <li><a href="http://museumvictoria.com.au/education/mvteachers/">Join MV teachers</a></li>
                            <li><a href="http://museumvictoria.com.au/footer-links/e-news/">Subscribe to MV E-News</a></li>
                        </ul>
                    </div>
                    <div class="footerpromo">
                        <h2>Links</h2>
                        <ul>
                            <li><a href="http://museumvictoria.com.au/about/work-opportunities/employment/">Employment</a></li>
                            <li><a href="http://museumvictoria.com.au/education/">Education</a></li>
                            <li><a href="http://museumvictoria.com.au/about/media-centre/">Media</a></li>
                            
                        </ul>
                    </div>
                </div> <!--footer promos -->
            </div>
            <div class="footerbase">
                <div class="wrapper">
                    <div class="venueinformation">
                        <strong>Museum Victoria</strong> &nbsp;&nbsp;|&nbsp;&nbsp; GPO Box 666 Melbourne
                        3001, Victoria, Australia &nbsp;&nbsp;|&nbsp;&nbsp; Bookings &amp; Enquiries 13
                        11 02
                        <ul>
                            <li class="Copyright">&copy; <strong>MUSEUM</strong> VICTORIA</li>
                            <li><a href="http://museumvictoria.com.au/footer-links/privacy/">Privacy</a></li>
                            <li><a href="http://museumvictoria.com.au/footer-links/disclaimer/">Disclaimer</a></li>
                            <li><a href="http://museumvictoria.com.au/footer-links/conditions-of-use/">Conditions</a></li>
                            <li><a href="http://museumvictoria.com.au/footer-links/rights/">Rights</a></li>
                            <li><a href="http://museumvictoria.com.au/footer-links/contact-us/">Contact us</a></li>
                        </ul>
                    </div>
                    <div class="stategovernmentlinks">
                        <a href="http://www.vic.gov.au/">
                            <img src="/content/img/logo-creative-victoria.png" alt="State Government Victoria" />
                        </a>
                    </div>
                </div>
            </div>
        </footer>
        <script src="/content/js/bundle.js"></script>
        <script type="application/ld+json">
        {
           "@@context": "http://schema.org",
           "@@type": "WebSite",
           "url": "http://collections.museumvictoria.com.au/",
           "potentialAction": {
             "@@type": "SearchAction",
             "target": "http://collections.museumvictoria.com.au/search?query={search_term_string}",
             "query-input": "required name=search_term_string"
           }
        }
        </script>
    </body>
</html>