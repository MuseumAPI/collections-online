@using CollectionsOnline.WebSite.Extensions
@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<CollectionsOnline.WebSite.Transformers.ItemViewTransformerResult>
@{
    Layout = "Layout.cshtml";
    var imageCount = 0;
    var audioCount = 0;
    var videoCount = 0;
    var bookCount = 0;
}
@if (Model.ItemImages.FirstOrDefault() != null)
{
    @Html.Partial("ImageMedia", Model.ItemImages.First())
}
<div  id="objectthumbs">
    @if (Model.ItemImages.Any())
    {
        imageCount = Model.ItemImages.Count();
        <div class="thumbnails wrapper">
            @foreach (var itemImage in Model.ItemImages)
            {
                <div class="thumbnail">
                    @if (itemImage.Thumbnail.Uri == Model.ItemImages.First().Thumbnail.Uri)
                    {
                        <img class="active" src="@itemImage.Thumbnail.Uri" alt="@itemImage.AlternativeText" />
                    }
                    else
                    {
                        <img src="@itemImage.Thumbnail.Uri" alt="@itemImage.AlternativeText" />
                    }
                </div>
            }
        </div>
    }

</div>
<div id="title">
    <div class="wrapper">
        <div class="mediatypes">
            <div class="type active images">
                <div class="icon-image"><p>Images (@imageCount.ToString())</p></div>
            </div>
            <div class="type videos">
                <div class="icon-video"><p>Videos (@videoCount.ToString())</p></div>
            </div>
            <div class="type audios">
                <div class="icon-audio"><p>Audio (@audioCount.ToString())</p></div>
            </div>
            <div class="type books">
                <div class="icon-book"><p>Books (@bookCount.ToString())</p></div>
            </div>
        </div>
        <h1>
            <span class="reg-number item">
                Item
                @Model.Item.RegistrationNumber            
            </span>
            </br>
            @Html.Raw(Model.Item.DisplayTitle)<br />
 
        </h1>
       
    </div>
</div>

