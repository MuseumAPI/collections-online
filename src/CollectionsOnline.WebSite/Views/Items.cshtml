                            <p>@Model.Item.ArtworkPlateNumber</p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.ArtworkDrawingNumber))
                    {
                        <li>
                            <h3>Drawing Number</h3>
                            <p>@Model.Item.ArtworkDrawingNumber</p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.ArtworkState))
                    {
                        <li>
                            <h3>State</h3>
                            <p>@Model.Item.ArtworkState</p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.ArtworkPrimaryInscriptions))
                    {
                        <li>
                            <h3>Primary Inscriptions</h3>
                            <p>@Model.Item.ArtworkPrimaryInscriptions</p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.ArtworkSecondaryInscriptions))
                    {
                        <li>
                            <h3>Secondary Inscriptions</h3>
                            <p>@Model.Item.ArtworkSecondaryInscriptions</p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.ArtworkTertiaryInscriptions))
                    {
                        <li>
                            <h3>Tertiary Inscriptions</h3>
                            <p>@Model.Item.ArtworkTertiaryInscriptions</p>
                        </li>
                    }
                    @if (@Model.Item.IndigenousCulturesCulturalGroups.Any())
                    {
                        <li>
                            <h3>Cultural Groups</h3>
                            <p>
                                @for (int i = 0; i < Model.Item.IndigenousCulturesCulturalGroups.Count; i++)
                                {
                                    <a href="/search?culturalgroup=@HttpUtility.UrlEncode(Model.Item.IndigenousCulturesCulturalGroups[i])">@Model.Item.IndigenousCulturesCulturalGroups[i]</a>@(i != Model.Item.IndigenousCulturesCulturalGroups.Count - 1 ? ", " : "")
                                }
                            </p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.IndigenousCulturesMedium))
                    {
                        <li>
                            <h3>Object/Medium</h3>
                            <p>@Model.Item.IndigenousCulturesMedium</p>
                        </li>
                    }
                    @if (Model.Item.Classifications.Any())
                    {
                        <li>
                            <h3>Classification</h3>
                            <p>
                                @for (int i = 0; i < Model.Item.Classifications.Count; i++)
                                {
                                    <a href="/search?classification=@HttpUtility.UrlEncode(Model.Item.Classifications[i])">@Model.Item.Classifications[i]</a>@(i != Model.Item.Classifications.Count - 1 ? ", " : "")
                                }
                            </p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.ScientificName))
                    {
                        <li>
                            <h3>Scientific Name</h3>
                            <p><a href="/search?taxon=@HttpUtility.UrlEncode(Model.Item.ScientificNameText)">@Html.Raw(Model.Item.ScientificName)</a></p>
                        </li>
                    }
                    @if (Model.Item.Taxonomy != null && !string.IsNullOrWhiteSpace(Model.Item.Taxonomy.Author))
                    {
                        <li>
                            <h3>Author and date of publication</h3>
                            <p>@Model.Item.Taxonomy.Author</p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.TypeStatus))
                    {
                        <li>
                            <h3>Type Status</h3>
                            <p>@Model.Item.TypeStatus</p>
                        </li>
                    }
                    @if (Model.Item.Taxonomy != null)
                    {
                        if (!string.IsNullOrWhiteSpace(Model.Item.Taxonomy.CommonName))
                        {
                            <li>
                                <h3>Preferred Common name</h3>
                                <p>@Model.Item.Taxonomy.CommonName</p>
                            </li>
                        }
                        if (Model.Item.Taxonomy.OtherCommonNames.Any())
                        {
                            <li>
                                <h3>Other Common Names</h3>
                                <p>
                                    @foreach (var otherCommonNames in @Model.Item.Taxonomy.OtherCommonNames)
                                    {
                                        <span>@otherCommonNames</span>
                                    }
                                </p>
                            </li>
                        }
                    }
                    @Html.Partial("Taxonomy", Model.Item.Taxonomy)                    
                    @if (!string.IsNullOrWhiteSpace(Model.Item.IdentifiedBy))
                    {
                        <li>
                            <h3>Identified By</h3>
                            <p>@Model.Item.IdentifiedBy</p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.DateIdentified))
                    {
                        <li>
                            <h3>Date Identified</h3>
                            <p>@Model.Item.DateIdentified</p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.Category))
                    {
                        <li>
                            <h3>Category</h3>
                            <p><a href="/search?category=@HttpUtility.UrlEncode(Model.Item.Category)">@Model.Item.Category</a></p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.Discipline))
                    {
                        <li>
                            <h3>Discipline</h3>
                            <p><a href="/search?collection=@HttpUtility.UrlEncode(@Model.Item.Discipline.ToLower())">@Model.Item.Discipline</a></p>
                        </li>

                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.Type))
                    {
                        <li>
                            <h3>Type of item</h3>
                            <p><a href="/search?itemtype=@HttpUtility.UrlEncode(@Model.Item.Type.ToLower())">@Model.Item.Type</a></p>
                        </li>
                    }
                    @foreach (var dimension in @Model.Item.Dimensions.Where(x => !string.IsNullOrWhiteSpace(x.Dimensions)))
                    {
                        <li>
                            <h3>
                                @if (!string.IsNullOrWhiteSpace(dimension.Configuration))
                                {
                                    @dimension.Configuration
                                }
                                else
                                {
                                    @:Dimensions
                                }
                            </h3>
                            <p>
                                @dimension.Dimensions<br />
                                @if (!string.IsNullOrWhiteSpace(dimension.Comments))
                                {
                                    @dimension.Comments<br />
                                }
                            </p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.ModelScale))
                    {
                        <li>
                            <h3>Model Scale</h3>
                            <p>@Model.Item.ModelScale</p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.Shape))
                    {
                        <li>
                            <h3>Shape</h3>
                            <p>@Model.Item.Shape</p>
                        </li>
                    }
                    @if (!string.IsNullOrWhiteSpace(Model.Item.References) || Model.Item.Bibliographies.Any())
                    {
                        <li>
                            <h3>References</h3>
                            <p>
                                @if (!string.IsNullOrWhiteSpace(Model.Item.References))
                                {
                                    @Html.ConvertHyperLinks(Model.Item.References)<br />
                                }
                                @foreach (var bibliography in Model.Item.Bibliographies)
                                {
                                    @bibliography<br />
                                }
                            </p>
                        </li>
                    }
                    @if (Model.Item.Keywords.Any())
                    {
                        <li>
                            <h3>Keywords</h3>
                            <p>
                                @for (int i = 0; i < Model.Item.Keywords.Count; i++)
                                {
                                    <a href="/search?keyword=@HttpUtility.UrlEncode(Model.Item.Keywords[i])">@Model.Item.Keywords[i]</a>@(i != Model.Item.Keywords.Count - 1 ? ", " : "")
                                }
                            </p>
                        </li>
                    }
                </ul>
            </div>
        </section>


            @if (Model.Item.MuseumLocation != null)
            {
          <aside class="display-content">              
                <div class="on-display">
                    On display<br />
                    <a href="/search?museumlocation=@HttpUtility.UrlEncode(@Model.Item.MuseumLocation.Gallery)">@Model.Item.MuseumLocation.Gallery</a><br />
                    <a href="/search?museumlocation=@HttpUtility.UrlEncode(@Model.Item.MuseumLocation.Venue)">@Model.Item.MuseumLocation.Venue</a>
                </div>
             </aside>           
            }

        <aside class="side-content">
            <div class="sidecolumn">
                <div class="social">
                    <h4>Share</h4>
                    <div class="social-tools">
                        <a class="icon" href="#">
                            <span class="icon-facebook" aria-hidden="true"></span>
                            <span class="icon-label-hidden">Facebook</span>
                        </a>
                        <a class="icon" href="#">
                            <span class="icon-twitter" aria-hidden="true"></span>
