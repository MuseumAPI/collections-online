<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="DatabaseUrl" value="http://zen.mv.vic.gov.au:8080/" />
    <add key="DatabaseName" value="collectionsonline_dev" />
    <add key="EmuServerHost" value="bunjil.mv.vic.gov.au" />
    <add key="EmuServerPort" value="40022" />
    <add key="MediaPath" value="" />
    <add key="MediaServerUri" value="/content/media" />
    <add key="SqlImportConnectionString" value="" />
    <add key="GoogleApiKey" value="" />
    <add key="OverwriteExistingMedia" value="false" />
    <add key="OverwriteExistingDocuments" value="false" />
  </appSettings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target name="file" xsi:type="File" layout="${longdate} [${level}] - ${message}" fileName="${basedir}\logs\log.txt" archiveFileName="${basedir}\logs\log.{#}.txt" archiveAboveSize="1048576" archiveNumbering="Sequence" maxArchiveFiles="9" />
      <target name="console" xsi:type="ColoredConsole" layout="${longdate} [${level}] - ${message}" useDefaultRowHighlightingRules="true" />
      <target name="mail" xsi:type="Mail" layout="${longdate} [${level}] - ${message}" html="true" subject="CollectionsOnline.Import Error" to="mmason@museum.vic.gov.au" from="webteam@museum.vic.gov.au" smtpServer="pop.mv.vic.gov.au" smtpPort="25" smtpAuthentication="none" />
    </targets>
    <rules>
      <logger name="*" minlevel="Error" writeTo="file,console" />
      <logger name="CollectionsOnline.*" minlevel="Trace" maxlevel="Warn" writeTo="file,console" />
    </rules>
  </nlog>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="MiniProfiler" publicKeyToken="b44f9351044011a3" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.10.0" newVersion="3.0.10.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>